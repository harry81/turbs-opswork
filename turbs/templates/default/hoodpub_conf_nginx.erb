upstream django-turbs {
    server unix:/tmp/uwsgi-turbs.sock;
}

server {
    listen      80;
    server_name .turbs.com;
    return  301 https://$server_name$request_uri;

    location /media  {
        alias /home/deploy/src/turbs/web/media/;
    }

    location /static {
        autoindex on;
        alias /home/deploy/src/turbs/web/static/;
    }

    location / {
        uwsgi_pass  django-turbs;
        include /etc/nginx/uwsgi_params;
    }
}
